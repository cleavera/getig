{
  "name": "@getig/docs",
  "version": "0.1.0",
  "description": "Documentation for getig",
  "scripts": {
    "build": "npm run clean && npm run lint && npm run compile",
    "dev": "chokidar src/**/* content/**/* -c \"npm run compile\"",
    "compile": "ts-node src/core/index",
    "clean": "git clean -X -f -d --exclude=\"!node_modules\"",
    "lint": "tslint --project ./tsconfig.json"
  },
  "author": "Anthony Cleaver",
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "@cleavera/fs": "^1.4.1",
    "@getig/common": "^0.5.0",
    "@getig/core": "^0.8.0",
    "@types/glob": "^7.1.1",
    "@types/markdown-it": "0.0.9",
    "@types/node": "^10.12.15",
    "@types/node-sass": "^4.11.0",
    "chokidar-cli": "^2.1.0",
    "glob": "^7.1.6",
    "markdown-it": "^10.0.0",
    "node-sass": "^4.13.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {}
}
